//
// This file defines the H2BLB-specific builtin function database. Users of
// this file must define the BUILTIN macro to make use of this information.
//
//===----------------------------------------------------------------------===//

// The format of this database matches clang/Basic/Builtins.def.

#if defined(BUILTIN) && !defined(TARGET_BUILTIN)
#   define TARGET_BUILTIN(ID, TYPE, ATTRS, FEATURE) BUILTIN(ID, TYPE, ATTRS)
#endif

// Widening signed multiply.
// Type: Returns one signed (S) int (i) and takes two signed (S) shorts (s).
// Attributes: NoThrow (n) and Const (c)
TARGET_BUILTIN(__builtin_h2blb_widening_smul, "SiSsSs", "nc", "")

// Widening unsignedsigned multiply.
// Type: Returns one unsigned (U) int (i) and takes two signed (U) shorts (s).
// Attributes: NoThrow (n) and Const (c)
TARGET_BUILTIN(__builtin_h2blb_widening_umul, "UiUsUs", "nc", "")

#undef BUILTIN
#undef TARGET_BUILTIN
